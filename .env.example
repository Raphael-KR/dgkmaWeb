# 카카오 로그인 설정
VITE_KAKAO_JS_KEY=your_kakao_javascript_key_here

# Supabase 데이터베이스 설정
# Supabase 프로젝트에서 "Connect" > "Connection string" > "Transaction pooler" 에서 복사
DATABASE_URL=postgresql://postgres.xxxxxxxxxxxxxxxxxxxx:[YOUR-PASSWORD]@aws-0-ap-northeast-2.pooler.supabase.com:6543/postgres
PGDATABASE=postgres
PGHOST=aws-0-ap-northeast-2.pooler.supabase.com
PGPASSWORD=your_supabase_password
PGPORT=6543
PGUSER=postgres.xxxxxxxxxxxxxxxxxxxx

# 구글 시트 API (동문 데이터 연동용)
GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key_here
GOOGLE_SHEET_ID=your_google_sheet_id_here

# 카카오톡 메시지 API (알림용)
KAKAO_ADMIN_KEY=your_kakao_admin_key_here

# JWT 비밀키
JWT_SECRET=your_jwt_secret_key_here

# 개발 환경 설정
NODE_ENV=development